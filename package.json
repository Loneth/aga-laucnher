{
  "name": "aga-launcher",
  "version": "1.0.1",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "publish": "build --win -p always"
  },
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^4.0.6"
  },
  "build": {
    "appId": "com.github.loneth.agalauncher",
    "publish": [
      {
        "provider": "github",
        "owner": "loneth",
        "repo": "aga-launcher"
      }
    ],
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
}
